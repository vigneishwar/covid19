{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid19-proj"
		},
		"ls_ablob_covid19_rg_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covid19_rg'"
		},
		"ls_adlg2_covid19_rg_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlg2_covid19_rg'"
		},
		"ls_adlg2_covid19_rg_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid1dl9.dfs.core.windows.net/"
		},
		"ls_http_ecdc_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.ecdc.europa.eu"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid19-proj_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covid19_rg_connectionString": {
						"value": "[parameters('ls_ablob_covid19_rg_connectionString')]"
					},
					"ls_adlg2_covid19_rg_accountKey": {
						"value": "[parameters('ls_adlg2_covid19_rg_accountKey')]"
					},
					"ls_adlg2_covid19_rg_properties_typeProperties_url": {
						"value": "[parameters('ls_adlg2_covid19_rg_properties_typeProperties_url')]"
					},
					"ls_http_ecdc_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_ecdc_eu_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}